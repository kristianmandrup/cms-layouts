include ./link-item.jade
mixin top-menu(activeItem)
  .ui.large.secondary.inverted.pointing.menu

    a.toc.item
      i.sidebar.icon

    each item in menus.main
      +link-item(item, activeItem)

    .right.item
      each item in menus.session
        a.ui.inverted.button(href='#{item.link}') #{item.label}
